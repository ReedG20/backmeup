{
  "name": "my-expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:types": "supabase gen types typescript --local > lib/database.types.ts",
    "functions:serve": "supabase functions serve --env-file .env --no-verify-jwt"
  },
  "dependencies": {
    "@expo/ui": "^0.2.0-beta.9",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react-native": "^1.0.10",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@siteed/expo-audio-studio": "^2.18.1",
    "@supabase/supabase-js": "^2.89.0",
    "buffer": "^6.0.3",
    "expo": "^54.0.0",
    "expo-blur": "^15.0.8",
    "expo-constants": "^18.0.12",
    "expo-glass-effect": "^0.1.8",
    "expo-linking": "^8.0.11",
    "expo-notifications": "~0.32.15",
    "expo-router": "^6.0.21",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.10",
    "nativewind": "latest",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "^4.19.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "~5.9.2"
  },
  "main": "expo-router/entry",
  "private": true
}
